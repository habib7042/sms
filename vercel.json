{
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "build": {
    "env": {
      "PRISMA_GENERATE_SKIP_CACHE": "true"
    }
  },
  "env": {
    "DATABASE_URL": "@database_url"
  }
}